<schema>
	<element name="itemaccount" tablename="itemaccount" >
		<field name="objid" primary="true"/>
		<field name="state" required="true"/>
		<field name="acctcode" />
		<field name="accttitle" required="true"/>
		<field name="fundid" required="true" />
		<field name="systype" /> <!-- BUSINESS_TAX, REG_FEE, OTHER_FEE, etc. -->
		<field name="valuetype" /> <!-- FIX, FIX_UNIT, NULL -->
		<field name="defaultvalue" type="decimal" />
	</element>
</schema>

