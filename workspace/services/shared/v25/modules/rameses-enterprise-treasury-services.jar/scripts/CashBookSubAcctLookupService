import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.common.*
import com.rameses.services.extended.*

class CashBookSubAcctLookupService extends ActiveListService {

	@ActiveDB("cashbook_subaccount")
	def em;

	@ProxyMethod
	public def getColumns( def p ) {
		return [
			[name:"name", caption:"Account Name"],
			[name:"subaccttype", caption:"Sub Account Type"],
			[name:"org.name", caption:"Org"],
		];	
	}	
	
	public void afterList(def p, def list) {
		list.each {
			it.name = it.firstname + " " + it.lastname + "(" + it.subaccttype + ")"; 
		}
	}
	
	
}
