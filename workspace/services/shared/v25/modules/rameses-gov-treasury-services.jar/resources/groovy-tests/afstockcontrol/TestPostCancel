def svc = new osiris3.groovy.tester.Service(
    ["app.context" : "etracs25", 
     "app.host": "localhost:8070",
     "app.cluster": "osiris3"] );

def ts = svc.lookup("AFStockService");

def m = [refid:'CANCEL01', reftype:'CANCEL', refdate:new Date(), startseries: 5000001, startstub:1];
m.remarks = "DESTROYRED";

//we will cancel 2 stubs. assume unitqty of 50
m.controlid = "IRAF00001-51";
m.qty = 2;
m.endseries = (m.startseries + (m.qty*50)) - 1;
m.endstub = m.startstub + m.qty - 1;
println m;

ts.postCancel( m );