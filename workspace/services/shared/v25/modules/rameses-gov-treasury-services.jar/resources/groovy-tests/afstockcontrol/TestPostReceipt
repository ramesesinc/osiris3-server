def svc = new osiris3.groovy.tester.Service(
    ["app.context" : "etracs25", 
     "app.host": "localhost:8070",
     "app.cluster": "osiris3"] );

def ts = svc.lookup("AFStockService");


//test add new items to inventory
def m = [items:[]];
m.refid = "IRAF00001";
m.reftype = "RECEIPT";
m.refdate = new Date();

def item1 =[ af: 51, qtyperunit: 50,startstub:1, unit: 'stub', startseries: 5000001, endseries: 5000300, prefix:'A', suffix:'B'] 
    item1.qty = (int) (((item1.endseries-item1.startseries) +1) / item1.qtyperunit);  
    item1.endstub = (int)(item1.startstub + item1.qty-1); 
m.items << item1;  
    
/*   
def item2 =[ af: 56, qtyperunit: 50,startstub:5, unit: 'stub', startseries: 6000001, endseries: 6000450, prefix:'A', suffix:'B'];
    item2.qty = (int)(((item2.endseries-item2.startseries) +1) / item2.qtyperunit);  
    item2.endstub = (int)(item2.startstub + item2.qty-1);
m.items << item2;  
