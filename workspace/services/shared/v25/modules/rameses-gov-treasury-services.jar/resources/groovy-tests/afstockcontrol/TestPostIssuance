def svc = new osiris3.groovy.tester.Service(
    ["app.context" : "etracs25", 
     "app.host": "localhost:8070",
     "app.cluster": "osiris3"] );

def ts = svc.lookup("AFStockQueryService");


def m = [af:'51', qty:2]
def items = ts.getAvailable( m );
items.each {
    println it
}

def z = [refid:'RIS0001',reftype:'ISSUE', refdate:new Date()];
z.issueto = [objid:'USR01', name:'ELMO', jobtitle:'COLLECTOR1'];
z.remarks = 'sent to collector emn';
z.items = items;

def ps = svc.lookup("AFStockService");
ps.postIssue( z );