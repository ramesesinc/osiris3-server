import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.common.*
import com.rameses.services.extended.*

class RevenueItemService extends AbstractCrudListService {

	@PersistenceContext("main")
	def em;

	String schemaName = "revenueitem";
	
	@ProxyMethod
	public def getColumns( def o ) {
		return[
			[name:'code', caption:'Item Code' ],
			[name:'title', caption:'Item Title' ],
			[name:'ngasacct', caption:'NGAS Account' ],
			[name:'fundacct', caption:'Fund Account' ],
		];
	}
	
		
}
